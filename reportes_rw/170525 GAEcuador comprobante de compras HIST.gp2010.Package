{
	CalculatedFields
	{
		CalculatedField	"Description"		
		{
			EvaluateAfter	""
			Expression	"'RW_GetDescription'( 2 'PM_Paid_Transaction_HIST'.'TRX Source''PM_Paid_Transaction_HIST'.'Voucher Number''PM_Paid_Transaction_HIST'.'Document Type' )  "
			ResultType	"String"
		}
		CalculatedField	"Direccion"		
		{
			EvaluateAfter	""
			Expression	"'STRIP'( 'PM_Vendor_MSTR'.'Address 1' )   #  \" \"  #  'STRIP'( 'PM_Vendor_MSTR'.'Address 2' )   #  \" \"  #  'STRIP'( 'PM_Vendor_MSTR'.'Address 3' )  "
			ResultType	"String"
		}
		CalculatedField	"Restriction"		
		{
			EvaluateAfter	""
			Expression	"'eBizS_PM_Inquiry_TEMP_CER_RET'.'Document Date'"
			ResultType	"Date"
		}
		CalculatedField	"TaxAmount"		
		{
			EvaluateAfter	""
			Expression	"'RW_eBizSGetTaxAmount'( 2 'PM_Paid_Transaction_HIST'.'TRX Source''PM_Paid_Transaction_HIST'.'Voucher Number' )  "
			ResultType	"Currency"
		}
		CalculatedField	"Total"		
		{
			EvaluateAfter	""
			Expression	"'TotalAmount'  +  'PM_Paid_Transaction_HIST'.'Tax Amount'"
			ResultType	"Currency"
		}
		CalculatedField	"TotalAmount"		
		{
			EvaluateAfter	""
			Expression	"'PM_Paid_Transaction_HIST'.'Purchases Amount' -  'PM_Paid_Transaction_HIST'.'Trade Discount Amount' +  'PM_Paid_Transaction_HIST'.'Freight Amount' +  'PM_Paid_Transaction_HIST'.'Misc Charges Amount'"
			ResultType	"Currency"
		}
		CalculatedField	"Valor IMP"		
		{
			EvaluateAfter	""
			Expression	"if 'PM_Tax_HIST'.'Tax Amount' <  0.00000 then 'PM_Tax_HIST'.'Tax Amount'else 0.00000 "
			ResultType	"Currency"
		}
		CalculatedField	"cal_EjercicioFiscal"		
		{
			EvaluateAfter	""
			Expression	"'DAT_YYYY'( 'PM_Paid_Transaction_HIST'.'Posting Date' )  "
			ResultType	"Integer"
		}
		CalculatedField	"cal_SerieSecuencial"		
		{
			EvaluateAfter	""
			Expression	"\"\" "
			ResultType	"String"
		}
		CalculatedField	"cal_TipoDoc"		
		{
			EvaluateAfter	""
			Expression	"\"\" "
			ResultType	"String"
		}
		CalculatedField	"cal_base"		
		{
			EvaluateAfter	""
			Expression	"if 'PM_Tax_HIST'.'Tax Detail ID' =  \"104-725-1\" then 'RW_Abs'( 'Valor IMP'  )  else 'PM_Tax_HIST'.'Tax Detail Total Purchases'"
			ResultType	"Currency"
		}
		CalculatedField	"cal_pctj"		
		{
			EvaluateAfter	""
			Expression	" (  'Valor IMP'  /  'cal_base'  )   *  100.00000 "
			ResultType	"Currency"
		}
		CalculatedField	"cal_pctj_str"		
		{
			EvaluateAfter	""
			Expression	"'CUR_STR'( 'cal_pctj'  )  "
			ResultType	"String"
		}
		CalculatedField	"cal_percent_ret"		
		{
			EvaluateAfter	""
			Expression	"if 'TX_Detail_MSTR'.'Tax Detail ID' =  \"104-725-1\" then 100 else 'TX_Detail_MSTR'.'Tax Detail Percent' *   (   -  1  )  "
			ResultType	"Currency"
		}
	}
