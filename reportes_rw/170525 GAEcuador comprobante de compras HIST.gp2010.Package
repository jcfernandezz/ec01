<Component Name="Comprobante de Compras HIST" ProductId="9890" Object="VBAReport" >
VBAReport	"Comprobante de Compras HIST"
{
	Code	"Option Explicit

Dim cn As New ADODB.Connection
Dim rst As New ADODB.Recordset
Dim cmd As New ADODB.Command

Private Sub Report_BeforeAH(ByVal Level As Integer, SuppressBand As Boolean)
On Error GoTo MSJERR

'Create a command to select all customers
cmd.ActiveConnection = cn
cmd.CommandType = 1     'adCmdText
cmd.CommandText = \"select td.nsacoa_description, trx.nsa_serie, trx.nsacoa_secuencial from NSACOA_GL00021 trx inner join nsacoa_gl00004 td on trx.nsa_tipo_comprob = td.nsa_tipo_comprob where trx.VENDORID = '\" + RTrim(VendorID.Value) + \"' and trx.DOCNUMBR = '\" + RTrim(VoucherNumber.Value) + \"'\"
Set rst = cmd.Execute

If Not (rst.EOF) Then  'And rst.BOF
'  MsgBox RTrim(rst!nsacoa_description)
  calTipoDoc = UCase(RTrim(rst!nsacoa_description))
  calSerieSecuencial = Left(RTrim(rst!nsa_serie), 3) + \"-\" + Right(RTrim(rst!nsa_serie), 3) + \"-\" + RTrim(rst!nsacoa_secuencial)

End If
rst.Close
    
'Do While Not (rst.EOF And rst.BOF)
'  MsgBox rst.Fields(0).Value
'  calTipoDoc = rst.Fields(0).Value
'Loop

MSJERR:
'MsgBox \"BeforeAH: err\"
End Sub

Private Sub Report_End()
On Error GoTo MSJERR

'close the connection
    If rst.State = adStateOpen Then rst.Close
    If cn.State = adStateOpen Then cn.Close
    Set cn = Nothing
    Set rst = Nothing
    Set cmd = Nothing
    
'MsgBox \"End: ok\"

MSJERR:

End Sub

Private Sub Report_Start()
On Error GoTo MSJERR
'Retrieve an ADO connection for the current user
Set cn = UserInfoGet.CreateADOConnection()

'Set the connection properties
cn.CursorLocation = 3   'adUseClient

'Set the current database, using the IntercompanyID property
cn.DefaultDatabase = UserInfoGet.IntercompanyID

'MsgBox \"Start: ok\"

MSJERR:
End Sub"
