          </QueryParameter>
          <QueryParameter Name="@p_Month">
            <Value>=Parameters!p_Month.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select 
	VCHRNMBR [Código de compra],
	formaPago [Forma de pago]
from dbo.gersVwComprasPagos   
